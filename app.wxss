/**app.wxss**/
@import "./styles//iconfont.wxss";
@import "./styles//main.wxss";

/* 初始化样式 */
page,view,text,swiper,swiper-item,image,navigator{
  padding: 0;    
  margin: 0;
  box-sizing: border-box;
}
page{
  /* 定义主题颜色 */
  --themeColor: #eb4450;
  /* 定义统一字体大小 */
  font-size: 28rpx;
  height: 100%;
}
image{
  width: 100%;
}
